i = 1
while True:
    print(i)
    if i >= 3:
        break
    i += 1

//

def persistence(n):
    count = 0
    while len(str(n)) > 1:
        n=int(str(n)[0])*int(str(n)[1])
        count+=1
    return count
   
print(persistence(999))

