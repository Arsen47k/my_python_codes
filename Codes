width = float(input(" Գրեք ուղղանկյան լայնությունը: "))
height = float(input(" Գրեք ուղղանկյան բարձրությունը: "))
perimeter = 2 * (width + height)
print(" ուղղանկյան պարագիծը:  "  , perimeter)

